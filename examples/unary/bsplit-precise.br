fix bsplit(z). Lam. lam l. caseL l of 
   nil => (nil, nil)
 | h::tl => caseL tl of
   	    nil => (h::nil, nil)
	  | h'::tl' => let x = bsplit () [] tl' in
	    	       (h :: fst x, h' :: snd x)	    

